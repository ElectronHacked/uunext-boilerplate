{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "components": "./src/components",
          "enums": "./src/enums",
          "layouts": "./src/layouts",
          "models": "./src/models",
          "redux-store": "./src/redux-store",
          "routes": "./src/routes",
          "styles": "./src/styles",
          "utils": "./src/utils",
        }
      }
    ],
    ["babel-plugin-styled-components"],
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ]
  ]
}